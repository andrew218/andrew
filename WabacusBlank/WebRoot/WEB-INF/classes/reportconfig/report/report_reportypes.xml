				<sql>
					  <select>
						<value>
							<![CDATA[select a.pid,orderNumber,order_type,request_date,sign_date,agent_user,is_take_off,order_status,customer_name,source,team_name,invite_peoper,interview_people,accept_user,contract_amount,fee_rate,deposit_money,expect_comm,bank_name,month_rate,year_term,repayment_modes from t_order_info a,t_accept_mortgage b where a.pid = b.pid and a.order_type = 10 and {#condition#} order by request_date]]>
						</value>
						<condition name="txtorderNumber" label="订单编号">
							<value>
								<![CDATA[(A.orderNumber like  '%#data#%' )]]>
							</value>
						</condition>
						<condition name="txtcustomer_name" label="客户姓名">
							<value>
								<![CDATA[(customer_name like  '%#data#%' )]]>
							</value>
						</condition>
					  </select>
					  <update>
					     update t_order_info(@{orderNumber},@{order_type},@{request_date},@{sign_date},@{agent_user},@{is_take_off},@{order_status},@{customer_name},@{source},@{team_name},@{invite_peoper},@{interview_people},@{accept_user},@{contract_amount},@{fee_rate},@{deposit_money},@{expect_comm},@{bank_name},@{month_rate},@{year_term},@{repayment_modes}) where pid = @{pid};
					     update t_accept_mortgage(RECE_DEPOSIT=@{deposit_money}) where pid = @{pid};
					  </update>
					</sql>
					<format>
					  <value>
							order_type=order_type==null?"10":order_type.trim();
							if(order_type.equals("10")) order_type="抵押单";
							else order_type="信用单";
							is_take_off=is_take_off==null?"1":is_take_off.trim();
							if(is_take_off.equals("1")) is_take_off="有";
							else is_take_off="无";
							order_status=order_status==null?"10":order_status.trim();
							if(order_status.equals("10")) order_status="等待受理";
							else if(order_status.equals("20")) order_status="受理面签";
							else if(order_status.equals("30")) order_status="待审批";
							else if(order_status.equals("40")) order_status="已出批复";
							else if(order_status.equals("90")) order_status="办理赎楼";
							else if(order_status.equals("100")) order_status="办理抵押";
							else if(order_status.equals("50")) order_status="放款收佣";
							else if(order_status.equals("60")) order_status="收款结案";
							else if(order_status.equals("70")) order_status="失败退费";
							else if(order_status.equals("80")) order_status="失败定转佣";
							
							source=source==null?"10":source.trim();
							if(source.equals("10")) source="内部经纪人";
							else if(source.equals("20")) source="外部经纪人";
							else if(source.equals("30")) source="员工推荐";
							else if(source.equals("40")) source="电台广告";
							else if(source.equals("50")) source="电销";
							else if(source.equals("60")) source="业主";
							else if(source.equals("70")) source="同行";
					  </value>
					</format>
			</report>
